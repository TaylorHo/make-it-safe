  "version": "0.1.4",
  "description": "Move vulnerabilities away. Make your project safer.",
  "main": "src/index.js",
  "bin": {
    "make-it-safer": "src/index.js",
    "makeitsafer": "src/index.js",
    "mis": "src/index.js"
